/* IN = {0, 1, 2, 3, 4}
 * OUT= {3, 4, 5, 6, 7}
 */
//This behaves as a vectorial fibonacci sequence calculator
transform MatrixVersionsTest 
from IN[n] 
through X<0..5>[n]
to OUT[n] 
{
  X<i>.cell(x) from(X<i-1>.cell(x) v, X<i-3>.cell(x) w){
    return v+w;
  }
  
  secondary X<i>.cell(x) from() {
    return 1;
  }

  //Init
  X<0>.cell(x) from(IN.cell(x) v){
    return v;
  }
  
  //Finish
  OUT.cell(x) from(X<5>.cell(x) v){
    return v;
  }
}
