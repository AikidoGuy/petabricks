
main transform Foo 
from A[n] 
to X
accuracy_metric Bar
accuracy_bins 1, 2, 3
{
  to(X x) from() {
    x = 0;
    for_enough(1; 5) { //min=1, max=5
      x=x+1;
    }
  }
}

transform Foo2
from A[n] 
to X
accuracy_metric Bar
accuracy_bins 1, 2, 3
{
  to(X x) from() {
    x = 0;
    for_enough {
      x=x+1;
    }
  }
}

transform Bar
to Accuracy
from X, A[n]
{
  Accuracy from() { return X; }
}


