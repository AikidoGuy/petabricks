#ifndef POISSON2DMGVTRAIN_PBCC
#define POISSON2DMGVTRAIN_PBCC

#include "Poisson2DForwardFunc.pbcc"
#include "Poisson2Dmgv.pbcc"
#include "mgv2DTrain.pbcc"

transform Poisson2DmgvTrain
from X[n,n], ACTUAL[n,n]
through zeros[(n+1)/2,(n+1)/2]
to Y[n,n]
{
  to (Y y) from (X x, ACTUAL actual)
  {
    mgv2DTrain(y, x, actual);
  }
}

#endif // POISSON2DMGVTRAIN_PBCC
