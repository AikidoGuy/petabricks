#ifndef JACOBIPRE_PBCC
#define JACOBIPRE_PBCC

transform JacobiPre
from A[n,n]
to P[n,n]
{
  to (P p) from (A a)
  {
	int i,j;
        for (i=0; i<n; i++) {
                for (j=0; j<n; j++) {
                        if (i == j && a.cell(i,j) != 0) {  p.cell(i,j) = 1/a.cell(i,j); }
                        else { p.cell(i,j) = 0; }
                }
        }
            
  }

 
}


#endif // JACOBIPRE_PBCC
